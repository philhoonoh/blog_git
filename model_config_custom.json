{
    "train" : {
        "seed": 42,
        "epochs": 50,
        "dataset": "MaskSplitByProfileDataset",
        "augmentation": "BasicAugmentation2",
        "resize": [224,224],
        "batch_size": 64,
        "valid_batch_size": 128,
        "model": "EfficientNet",
        "optimizer": "Adam",
        "lr": 1e-4,
        "lr_scheduler": "MultiStepLR",
        "lr_sch_params" : {
            "milestones" : [2,4,6,8],
            "gamma" : 0.5
         },
        "val_ratio": 0.2,
        "criterion": "cross_entropy",
        "lr_decay_step": 20,
        "log_interval": 20,
        "data_dir": "/opt/ml/input/data/train/images",
        "model_dir": "./model",
        "early_stopping" : 5 
    }
}
